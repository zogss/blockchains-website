<main class="d-flex flex-column" style="background-color: rgb(29, 31, 32);">
  {{! carousel }}
  <section
    class="carousel-section-block d-flex align-items-center justify-content-center"
    style="max-height: 100vh;"
  >
    <div
      id="carouselExampleIndicators"
      class="carousel slide w-100"
      data-bs-ride="true"
    >
      <div class="text-white position-absolute scroll-indicator">
        <svg
          xmlns="http://www.w3.org/2000/svg"
          width="25"
          height="25"
          fill="currentColor"
          class="bi bi-chevron-down"
          viewBox="0 0 16 16"
        >
          <path
            fill-rule="evenodd"
            d="M1.646 4.646a.5.5 0 0 1 .708 0L8 10.293l5.646-5.647a.5.5 0 0 1 .708.708l-6 6a.5.5 0 0 1-.708 0l-6-6a.5.5 0 0 1 0-.708z"
          ></path>
        </svg>

      </div>
      <div class="carousel-indicators">
        <button
          type="button"
          data-bs-target="#carouselExampleIndicators"
          data-bs-slide-to="0"
          class="active slide-button"
          aria-current="true"
          aria-label="Slide 1"
        ></button>
        <button
          type="button"
          data-bs-target="#carouselExampleIndicators"
          data-bs-slide-to="1"
          class="slide-button"
          aria-label="Slide 2"
        ></button>
        <button
          type="button"
          data-bs-target="#carouselExampleIndicators"
          data-bs-slide-to="2"
          class="slide-button"
          aria-label="Slide 3"
        ></button>
        <button
          type="button"
          data-bs-target="#carouselExampleIndicators"
          data-bs-slide-to="3"
          class="slide-button"
          aria-label="Slide 4"
        ></button>
      </div>
      <div class="carousel-inner">
        <div
          class="carousel-item active justify-center carousel-img carousel-img-1"
        >
          <div
            class="carousel-text-block carousel-caption justify-content-start"
          >
            <h6
              class="welcome-title text-start text-uppercase text-white text-opacity-75"
            >LAWYERS, TECHNOLOGISTS, AND ENTREPRENEURS</h6>
            <p class="welcome-text text-start text-white fw-bold">The Blockchain
              &
              <br />Cruptocurrency Law Firm</p>
            <p class="welcome-description text-start text-white fw-semibold">
              We provide a diverse set of services within blockchain technology
              to our clients across a myriad of industries and jurisdictions.
            </p>
            <div class="w-100 d-flex justify-content-start">
              <a
                href="/contact"
                class="fw-semibold btn btn-primary rounded-1"
              >Get A Consultation</a>
            </div>
          </div>
        </div>
        <div class="carousel-item justify-center carousel-img carousel-img-2">
          <div
            class="carousel-text-block carousel-caption justify-content-start"
          >
            <h6
              class="welcome-title text-start text-uppercase text-white text-opacity-75"
            >LAWYERS, TECHNOLOGISTS, AND ENTREPRENEURS</h6>
            <p class="welcome-text text-start text-white fw-bold">
              ENERGY & INFRASTRUCTURE
            </p>
            <p class="welcome-description text-start text-white fw-semibold">
              We help clients drive towards carbon neutrality, ESG politics and
              Sustainability Disclosure in order to promote a better business
              environment with more inclusion and diversity.
            </p>
            <div class="w-100 d-flex justify-content-start">
              <a
                href="/contact"
                class="fw-semibold btn btn-primary rounded-1"
              >Get A Consultation</a>
            </div>
          </div>
        </div>
        <div class="carousel-item justify-center carousel-img carousel-img-1">
          <div
            class="carousel-text-block carousel-caption justify-content-start"
          >
            <h6
              class="welcome-title text-start text-uppercase text-white text-opacity-75"
            >LAWYERS, TECHNOLOGISTS, AND ENTREPRENEURS</h6>
            <p class="welcome-text text-start text-white fw-bold">
              FINANCE
            </p>
            <p
              class="welcome-description text-start text-white fw-semibold pb-4"
            >
              From strategic consultancy to epic fusions. A huge board with more
              than 70 team members around the globe.
            </p>
            <div class="w-100 d-flex justify-content-start">
              <a
                href="/contact"
                class="fw-semibold btn btn-primary rounded-1"
              >Get A Consultation</a>
            </div>
          </div>
        </div>
        <div class="carousel-item justify-center carousel-img carousel-img-2">
          <div
            class="carousel-text-block carousel-caption justify-content-start"
          >
            <h6
              class="welcome-title text-start text-uppercase text-white text-opacity-75"
