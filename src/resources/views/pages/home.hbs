  {{! contact }}
  <section
    class="d-flex min-vh-100 justify-content-center align-items-center px-md-1 px-2 py-5 bg-white"
  >
    <div
      class="d-flex flex-column gap-5 justify-content-center align-items-center"
    >
      <div
        class="d-flex flex-column gap-3 justify-content-center align-items-center"
        data-animation="up"
      >
        <p class="fs-6 text-primary fw-light">WRITE A MESSAGE </p>
        <p class="fs-1 fw-bold">Get a 30 minute consultation</p>
        <p class="fs-4 text-secondary fw-normal">Please complete the form below
          and we’ll be in touch.</p>
      </div>
      <form
        class="row g-3 needs-validation w-100"
        data-animation="up"
        novalidate
      >
        <div class="col-md-12">
          <label for="validationCustom01" class="form-label pt-3">Your name</label>
          <input
            type="text"
            class="form-control bg-secondary bg-opacity-10 border-0 rounded-1 py-2"
            id="validationCustom01"
            placeholder="John Doe"
            required
          />
        </div>
        <div class="col-md-6">
          <label for="validationCustom02" class="form-label pt-3">Email address
            *</label>
          <input
            type="email"
            class="form-control bg-secondary bg-opacity-10 border-0 rounded-1 py-2"
            id="validationCustom02"
            placeholder="example@domain.com"
            required
          />
          <div class="invalid-feedback">
            Please provide a valid email.
          </div>
        </div>
        <div class="col-md-6">
          <label for="validationCustom03" class="form-label pt-3">Phone</label>
          <input
            type="text"
            class="form-control bg-secondary bg-opacity-10 border-0 rounded-1 py-2"
            id="validationCustom03"
            placeholder="+1-999-999-9999"
            required
          />
          <div class="invalid-feedback">
            Please provide a phone.
          </div>
        </div>
        <div class="col-md-12">
          <label
            for="validationCustom05"
            class="form-label pt-3"
          >Message</label>
          <textarea
            class="form-control bg-secondary bg-opacity-10 border-0 rounded-1 py-2"
            id="validationCustom05"
            aria-label="With textarea"
            placeholder="Tell us briefly about your needs"
            style="height: 120px;"
          ></textarea>
          <div class="invalid-feedback">
            Please provide a valid zip.
          </div>
        </div>
        <div class="col-12 pt-3">
          <button
            class="fw-bolder fs-6 btn btn-primary rounded-1 px-4 py-2"
            type="submit"
          >Submit</button>
        </div>
      </form>
